# Environment Variables Template
# 1. Copy this file to .env.local
# 2. Replace the placeholder values with your actual credentials
# 3. Never commit .env.local to version control

# Midweave Client Credentials
NEXT_PUBLIC_MW_CLIENT_ID=your_client_id_here
NEXT_PUBLIC_MW_CLIENT_SECRET=your_client_secret_here

# GitHub Configuration
NEXT_PUBLIC_GITHUB_TOKEN=your_github_token_here
NEXT_PUBLIC_REPOSITORY=your_username/your_repo
NEXT_PUBLIC_BRANCH=main

# OpenAI Configuration
NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Midweave Access Token
NEXT_PUBLIC_MW_ACCESS_TOKEN=your_access_token_here